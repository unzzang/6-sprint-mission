{
  "compilerOptions": {
    "target": "es2016",
    "module": "nodenext",
    "rootDir": "./src",
    "outDir": "./dist",
    "typeRoots": ["./types", "./node_modules/@types"],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
